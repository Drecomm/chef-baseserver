{
    "id": "deployer",
    "password": false,
    "ssh_keys": [
        "ssh-rsa  vagrant insecure public key"
    ],
    "groups": [ "www-data" ],
    "uid": 2001,
    "gid": 2001,
    "shell": "\/bin\/bash",
    "comment": "Deployer",
    "files": {
        /*
         Files that live in users cookbook for the user. For example,
         dotfiles. Note that in order for Chef to find dot files, they
         can't start with a dot character. They have the following
         structure:
         ".file_under_$HOME": {
         "mode": "0644",
         "source": "dot_file"
         }
         */
        // Example:
        ".gemrc": {
            "mode": "0644",
            "source": "dot-gemrc"
        }
    }
}